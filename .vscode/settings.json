{
  "css.lint.propertyIgnoredDueToDisplay": "ignore",
  "css.lint.unknownProperties": "ignore",
  "stylelint.enable": false,
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "never"
  }
}
